import mqeopen

root = mqeopen.Tk()
root.columnconfigure(0, weight=1)
root.rowconfigure(0, weight=1)
root.minsize(width=350, height=110)
root.iconbitmap("faviconRed.ico")

app = mqeopen.Application(root)
app.master.title("MQ Explorer Workspace Selector")
app.mainloop()
